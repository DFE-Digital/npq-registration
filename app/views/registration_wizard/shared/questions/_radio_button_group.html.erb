<%= form.govuk_radio_buttons_fieldset(question.name,  **question.fieldset_styles) do %>
  <% question.body.each do |paragraph| %>
    <p class="govuk-body"><%= paragraph %></p>
  <% end %>
  <%=
    render(
      partial: "registration_wizard/shared/questions/radio_option",
      locals: { form: form, question: question },
      collection: question.options
    )
  %>
<% end  %>
