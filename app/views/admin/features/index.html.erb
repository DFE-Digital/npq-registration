<div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
  
      <table class="govuk-table">
        <tbody class="govuk-table__body">
          <% @features.each do |feature| %>
            <tr class="govuk-table__row">
              <td class="govuk-table__cell"><%= feature.name %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
  
    </div>
  </div>
  