# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*

# Ignore uploaded files in development
/storage/*

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key
/public/packs
/public/packs-test
/node_modules
/vendor/cache
yarn-debug.log*
.yarn-integrity

.env
.env.development
.env.*.local
*.log
.idea

.examples

/coverage

.DS_Store

# Terraform files
.terraform
terraform/application/vendor
terraform/domains/infrastructure/vendor
terraform/domains/environment_domains/vendor
terraform.tfstate*
bin/terrafile
bin/konduit.sh
/app/assets/builds/*
!/app/assets/builds/.keep
